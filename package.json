{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && node ./bin/www",
    "server": "tsc && node ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "setup": "tsc && node ./build/setup/createTable.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "aws-sdk": "^2.944.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "log4js": "^6.3.0",
    "memorystore": "^1.6.6",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "concurrently": "^7.0.0",
    "typescript": "^4.3.5"
  }
}
